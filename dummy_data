INSERT INTO exercises(name,tag)
	VALUES("curls", "weight lifting");

INSERT INTO bodyParts(name)
	VALUES("biceps");

INSERT INTO bodyPartExerciseJoins(exercise_id, bodyPart_id)
	VALUES((SELECT id FROM exercises where name = "curls"), (SELECT id FROM bodyParts where name = "biceps"));

